<Application x:Class="Else.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             ShutdownMode="OnExplicitShutdown"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             d1p1:Ignorable="d"
             xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:converter="clr-namespace:Else.Converter">
	<Application.Resources>
		<ResourceDictionary>

			<!--Default style-->
			<ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/PresentationFramework.Aero2;V4.0.0.0;31bf3856ad364e35;component\themes/aero2.normalcolor.xaml" />

                <ResourceDictionary Source="Resources/Generic.xaml" />
                <ResourceDictionary Source="Resources/Styles.xaml" />
			</ResourceDictionary.MergedDictionaries>

			<!--Converters-->
			<converter:EqualsConverter x:Key="EqualsConverter2" />
			<converter:LastItemInContainerConverter x:Key="LastItemInContainerConverter" />
		</ResourceDictionary>
	</Application.Resources>
</Application>